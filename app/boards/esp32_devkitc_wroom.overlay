 / {
	shift_register {
		compatible = "gpio-keys";
		sr_data: sr_data {
			gpios = <&gpio0 15 GPIO_ACTIVE_HIGH>;
			label = "SR Data";
		};
		sr_latch: sr_latch {
			gpios = <&gpio0 16 GPIO_ACTIVE_HIGH>;
			label = "SR Latch";
		};
		sr_clock: sr_clock {
			gpios = <&gpio0 17 GPIO_ACTIVE_HIGH>;
			label = "SR clock";
		};
		sr_out_enable: sr_out_enable {
			gpios = <&gpio0 18 GPIO_ACTIVE_HIGH>;
			label = "SR Out Enable";
		};
		sr_clear: sr_clear {
			gpios = <&gpio0 19 GPIO_ACTIVE_HIGH>;
			label = "SR Clear";
		};
	};
